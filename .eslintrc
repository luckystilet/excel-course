{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "max-len": [2, {"code": 120, "tabWidth": 4, "ignoreUrls": true}],
    "guard-for-in": "off",
    "no-debugger": "off",
    "no-invalid-this": "off",
//   for development
    "no-unused-vars": "off",
    "no-case-declarations": "off",
    "no-prototype-builtins": "off",
    "no-empty": "off",
    "no-trailing-spaces": "off",
    "no-array-constructor": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
